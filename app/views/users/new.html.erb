<%= form_for(@user) do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>

  <p>
    <%= f.label :birthday %><br>
    <%= f.date_field :birthday %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
